<Window x:Class="GameOfThronesTournamentWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameOfThronesTournamentWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="995.588">
    <Grid Background="Black">
        <ListView x:Name="listView" HorizontalAlignment="Left" Height="205" Margin="27,91,0,0" VerticalAlignment="Top" Width="463">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
        <Button x:Name="list_houses_button" HorizontalAlignment="Left" VerticalAlignment="Top" Content="List Houses" Margin="27,66,0,0" Click="displayHouses"/>
        <Label x:Name="label" Content="Game of Thrones" HorizontalAlignment="Left" Height="39" Margin="193,10,0,0" VerticalAlignment="Top" Width="102" Foreground="White"/>
        <local:EditHouses HorizontalAlignment="Left" Margin="505,91,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
